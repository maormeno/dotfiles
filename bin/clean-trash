#!/usr/bin/env bash

# Empties the trash on all mounted volumes.
# > Also, clears Appleâ€™s system logs to improve shell startup speed.
#
# Author: Artem Sapegin, sapegin.me
# License: MIT
# https://github.com/sapegin/dotfiles

sudo command rm -rf ~/.Trash/*
sudo command rm -rfv /Volumes/*/.Trashes
sudo command rm -rfv /private/var/log/asl/*.asl
